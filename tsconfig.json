{
    "compileOnSave": false,
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": true,
        "noImplicitAny": false,
        "sourceMap": true,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "./build"
    },
    "filesGlob": [
        "**/*.ts",
        "!build/**",
        "!node_modules/**"
    ],
    "files": [
        "src/compiler.ts",
        "src/data.ts",
        "src/data/ArrayMap.ts",
        "src/data/ArraySeq.ts",
        "src/data/ArraySet.ts",
        "src/data/ArrayVector.ts",
        "src/data/Cons.ts",
        "src/data/Ident.ts",
        "src/data/interfaces.ts",
        "src/data/Keyword.ts",
        "src/data/proto.ts",
        "src/env.ts",
        "src/gensym.ts",
        "src/is.ts",
        "src/namespace.ts",
        "src/protocols.ts",
        "src/reader.ts",
        "src/specials.ts",
        "src/StringBuffer.ts",
        "test/compiler_test.ts",
        "test/protocols_test.ts",
        "test/reader_test.ts",
        "typings/assertion-error/assertion-error.d.ts",
        "typings/chai/chai.d.ts",
        "typings/mocha/mocha.d.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
